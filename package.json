{
  "name": "elmag-insurance-website",
  "version": "1.0.0",
  "description": "El-Mag Insurance Medicare Advantage programmatic website",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "test": "jest",
    "claude": "npx @anthropic-ai/claude-code",
    "claude-mcp": "npx @anthropic-ai/claude-code --mcp-config claude-desktop-config.json --model claude-3-5-sonnet-20241022 --permission-mode acceptEdits",
    "db:setup": "node scripts/setup-database.js",
    "db:test": "node scripts/test-database-connection.js",
    "db:backup": "node scripts/database-backup.js",
    "email:test": "node scripts/test-email-service.js",
    "email:monitor": "node scripts/monitor-email-service.js",
    "deploy:vercel": "bash scripts/deploy-vercel.sh",
    "test:production": "node scripts/post-deployment-test.js",
    "test:production:complete": "node scripts/comprehensive-production-test.js",
    "test:production:performance": "node scripts/lighthouse-audit.js",
    "verify:deployment": "node scripts/post-deployment-test.js elmag-insurance.com",
    "audit:lighthouse": "node scripts/lighthouse-audit.js elmag-insurance.com",
    "audit:mobile": "node scripts/lighthouse-audit.js elmag-insurance.com --mobile",
    "audit:desktop": "node scripts/lighthouse-audit.js elmag-insurance.com --desktop",
    "test:smoke": "bash scripts/production-smoke-test.sh",
    "test:full": "npm run test:smoke && npm run test:production:complete && npm run audit:lighthouse"
  },
  "keywords": [
    "insurance",
    "medicare",
    "healthcare",
    "next.js"
  ],
  "author": "El-Mag Insurance",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.83",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.3.0",
    "@jschuller/perplexity-mcp": "^2.0.1",
    "@modelcontextprotocol/sdk": "^1.17.3",
    "@prisma/client": "^6.15.0",
    "@react-email/render": "^1.2.1",
    "@types/node": "^20.12.0",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.542.0",
    "next": "^14.2.0",
    "nodemailer": "^7.0.5",
    "perplexity-mcp-server": "^1.2.1",
    "pg": "^8.16.3",
    "postcss": "^8.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.51.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.0",
    "vercel": "^25.2.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^15.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^29.7.0",
    "prisma": "^6.15.0"
  }
}
