    "tracking_infrastructure": {
      "google_analytics_4": {
        "property_id": "GA4_PROPERTY_ID",
        "measurement_id": "G-MEASUREMENT_ID",
        "enhanced_ecommerce": true,
        "custom_events": [
          {
            "event_name": "trip_cancellation_quote_start",
            "parameters": ["trip_cost", "destination", "travel_date"]
          },
          {
            "event_name": "trip_cancellation_form_submit",
            "parameters": ["form_type", "trip_value", "user_segment"]
          },
          {
            "event_name": "trip_cancellation_phone_click",
            "parameters": ["page_location", "user_segment", "traffic_source"]
          },
          {
            "event_name": "trip_cancellation_calculator_complete",
            "parameters": ["calculated_cost", "trip_type", "coverage_type"]
          }
        ],
        "custom_dimensions": [
          {
            "dimension_name": "trip_type",
            "scope": "event",
            "description": "Type of trip (cruise, international, domestic, business)"
          },
          {
            "dimension_name": "user_segment",
            "scope": "user",
            "description": "User classification based on behavior and demographics"
          },
          {
            "dimension_name": "lead_quality_score",
            "scope": "event",
            "description": "Calculated lead quality score 0-100"
          },
          {
            "dimension_name": "content_category",
            "scope": "event",
            "description": "Content type that led to conversion"
          }
        ]
      },
      "google_tag_manager": {
        "container_id": "GTM-XXXXXXXX",
        "triggers": [
          {
            "trigger_name": "Quote Form Submission",
            "trigger_type": "form_submit",
            "form_selector": "#trip-cancellation-quote-form"
          },
          {
            "trigger_name": "Phone Number Click",
            "trigger_type": "click",
            "element_selector": "a[href^='tel:']"
          },
          {
            "trigger_name": "Email Signup",
            "trigger_type": "form_submit",
            "form_selector": ".email-signup-form"
          },
          {
            "trigger_name": "Calculator Completion",
            "trigger_type": "custom_event",
            "event_name": "calculator_complete"
          },
          {
            "trigger_name": "Deep Scroll",
            "trigger_type": "scroll_depth",
            "scroll_percentage": 75
          }
        ],
        "variables": [
          {
            "variable_name": "Trip Cost Value",
            "variable_type": "form_field",
            "field_name": "trip_cost"
          },
          {
            "variable_name": "User Segment",
            "variable_type": "javascript",
            "javascript_code": "function(){return window.userSegment || 'unknown';}"
          },
          {
            "variable_name": "Lead Quality Score",
            "variable_type": "javascript",
            "javascript_code": "function(){return window.tripCancellationTracking?.getLeadScore() || 0;}"
          }
        ]
      },
