          "alert_threshold": "280%"
        },
        {
          "metric": "Lead Quality Score",
          "calculation": "weighted_average_lead_score",
          "target": "65+",
          "alert_threshold": "55"
        },
        {
          "metric": "Attribution Model Performance",
          "calculation": "model_specific_roas_comparison",
          "target": "data_driven_model_superiority",
          "review_frequency": "monthly"
