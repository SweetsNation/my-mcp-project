{
  "emergency_evacuation_analytics_dashboard": {
    "overview": {
      "dashboard_name": "Emergency Evacuation Insurance Performance Analytics",
      "update_frequency": "Real-time with 15-minute refresh intervals",
      "data_retention": "26 months for detailed analytics, lifetime for aggregate reporting",
      "access_level": "Marketing team, management, and authorized stakeholders",
      "integration_platforms": ["Google Analytics 4", "Google Ads", "Facebook Analytics", "Custom Analytics API"]
    },

    "key_performance_indicators": {
      "conversion_metrics": [
        {
          "metric": "Quote Request Rate",
          "calculation": "(Quote Requests / Page Views) * 100",
          "target": "4.2%",
          "current_benchmark": "3.8%",
          "alert_threshold": "< 3.0%",
          "widget_type": "gauge_chart",
          "priority": "high"
        },
        {
          "metric": "Phone Call Conversion Rate",
          "calculation": "(Phone Calls / Page Views) * 100",
          "target": "2.1%",
          "current_benchmark": "1.8%",
          "alert_threshold": "< 1.2%",
          "widget_type": "line_chart",
          "priority": "high"
        },
        {
          "metric": "Cost Per Acquisition (CPA)",
          "calculation": "Total Ad Spend / Total Conversions",
          "target": "$75",
          "current_benchmark": "$85",
          "alert_threshold": "> $120",
          "widget_type": "metric_card",
          "priority": "critical"
        },
        {
          "metric": "Return on Ad Spend (ROAS)",
          "calculation": "(Conversion Value / Ad Spend) * 100",
          "target": "400%",
          "current_benchmark": "350%",
          "alert_threshold": "< 250%",
          "widget_type": "trend_chart",
          "priority": "critical"
        }
      ],
      "engagement_metrics": [
        {
          "metric": "Average Session Duration",
          "calculation": "Total Session Time / Total Sessions",
          "target": "3:30",
          "current_benchmark": "2:45",
          "alert_threshold": "< 2:00",
          "widget_type": "metric_card",
          "priority": "medium"
        },
        {
          "metric": "Pages Per Session",
          "calculation": "Total Pageviews / Total Sessions",
          "target": "2.8",
          "current_benchmark": "2.3",
          "alert_threshold": "< 2.0",
          "widget_type": "bar_chart",
          "priority": "medium"
        },
        {
          "metric": "Bounce Rate",
          "calculation": "(Single Page Sessions / Total Sessions) * 100",
          "target": "< 40%",
          "current_benchmark": "45%",
          "alert_threshold": "> 60%",
          "widget_type": "gauge_chart",
          "priority": "medium"
        },
        {
          "metric": "Scroll Depth 75%+",
          "calculation": "(Users Scrolling 75%+ / Total Users) * 100",
          "target": "35%",
          "current_benchmark": "28%",
          "alert_threshold": "< 20%",
          "widget_type": "funnel_chart",
          "priority": "medium"
        }
      ],
      "technical_metrics": [
        {
          "metric": "Page Load Time (LCP)",
          "calculation": "Average Largest Contentful Paint",
          "target": "< 2.5s",
          "current_benchmark": "2.8s",
          "alert_threshold": "> 4.0s",
          "widget_type": "histogram",
          "priority": "high"
        },
        {
          "metric": "First Input Delay (FID)",
          "calculation": "Average First Input Delay",
          "target": "< 100ms",
          "current_benchmark": "85ms",
          "alert_threshold": "> 300ms",
          "widget_type": "metric_card",
          "priority": "high"
        },
        {
          "metric": "Cumulative Layout Shift (CLS)",
          "calculation": "Average Layout Shift Score",
          "target": "< 0.1",
          "current_benchmark": "0.08",
          "alert_threshold": "> 0.25",
          "widget_type": "gauge_chart",
          "priority": "high"
        }
      ]
    },

    "conversion_funnel_analysis": {
      "funnel_stages": [
        {
          "stage": "Landing Page Views",
